{
    "name": "Student Discounts",
    "version": "1.0.0",
    "description": "",
    "manifest_version": 3,
    "author": "",
    "action": {
        "default_popup": "index.html",
        "default_title": "Student Discounts"
    }
    "permissions": [
    "activeTab",
    "<all_urls>",
    "tabs"
  ], 
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["app.js"],
      "css": ["coupon.css"],
      "run_at": "document_end"
    }
  ]
}
